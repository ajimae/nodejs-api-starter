    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv-extended": "^2.8.0",
    "eslint-plugin-security": "^1.4.0",
