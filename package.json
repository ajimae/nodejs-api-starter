    "awilix": "^4.2.6",
    "awilix-express": "^3.0.0",
    "aws-sdk": "^2.716.0",
    "axios": "^0.19.2",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
